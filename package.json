{
  "name": "place-puppy",
  "version": "1.1.1",
  "description": "",
  "main": "dist/server/server.js",
  "scripts": {
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "webpack",
    "build:server": "babel src --ignore src/client/**/* --out-dir dist && cp package.json dist && npm install --only=production --prefix dist",
    "clean": "rm -rf dist",
    "start": "concurrently --kill-others \"webpack --watch\" \"AWS_PROFILE=place-puppy PUBLIC_PATH=dist/public babel-watch ./src/local-server.js\"",
    "install": "find ./node_modules/* -mtime +10950 -exec touch {} \\;",
    "package": "zip -r ./dist/place-puppy-$npm_package_version.zip dist",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && npm run build && npm run package && npm run deploy:package && npm run deploy:lambda",
    "deploy:package": "aws s3 cp ./dist/place-puppy-$npm_package_version.zip s3://place-puppy-mgmt/artifacts/place-puppy-$npm_package_version.zip",
    "deploy:lambda": "aws lambda update-function-code --function-name place-puppy --s3-bucket place-puppy-mgmt --s3-key artifacts/place-puppy-$npm_package_version.zip"
  },
  "author": "AJ Rabe <aj.rabe@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/seibar/place-puppy.git",
  "dependencies": {
    "aws-sdk": "2.372.0",
    "aws-serverless-express": "3.3.5",
    "express": "4.16.2",
    "jimp": "0.6.0",
    "node-fetch": "1.7.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babel-watch": "2.0.7",
    "concurrently": "3.5.1",
    "uglify-js-es6": "2.8.9",
    "uglifyes-webpack-plugin": "0.4.3",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.5"
  }
}
